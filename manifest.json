{
    "manifest_version": 3,
    "name": "TabBuddy",
    "description": "TabBuddy saves your tabs for later use.",
    "version": "1.0",         
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Extension Test!"
    },    
    "permissions": [         
        "tabs",
        "storage"
    ]
}